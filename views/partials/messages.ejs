<% if(typeof errors!= 'undefined') { %>
<%    errors.forEach(function(error){ %>
      <div class="error"> <%= error.msg %></div> 
<%    })        %>
<% } %>

<% if(success_msg!= '') { %>
        <p><%=success_msg %> </p>
      <% } %>

<% if(error_msg != '') { %>
      <div class="error"><%= error_msg %></div>
      <% } %> 
<% if(error != '') { %>
       <p><%= error %> </p>
      <% } %> 